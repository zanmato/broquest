{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "url",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "url_line"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": "\n"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "url_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "protocol"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "domain_and_port"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "url_components"
        }
      ]
    },
    "url_components": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "path"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "query_string"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "protocol": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "http"
            },
            {
              "type": "STRING",
              "value": "https"
            },
            {
              "type": "STRING",
              "value": "ftp"
            },
            {
              "type": "STRING",
              "value": "ws"
            },
            {
              "type": "STRING",
              "value": "wss"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "://"
        }
      ]
    },
    "domain_and_port": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "domain"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": ":"
                },
                {
                  "type": "SYMBOL",
                  "name": "port"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "domain": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "hostname"
        },
        {
          "type": "PATTERN",
          "value": "[a-zA-Z0-9]([a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?(\\.[a-zA-Z0-9]([a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?)*"
        }
      ]
    },
    "hostname": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "localhost"
        },
        {
          "type": "PATTERN",
          "value": "[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}"
        }
      ]
    },
    "port": {
      "type": "PATTERN",
      "value": "\\d{1,5}"
    },
    "path": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "variable"
              },
              {
                "type": "SYMBOL",
                "name": "path_param"
              },
              {
                "type": "SYMBOL",
                "name": "path_segment"
              },
              {
                "type": "STRING",
                "value": "/"
              }
            ]
          }
        }
      ]
    },
    "path_segment": {
      "type": "PATTERN",
      "value": "[^\\/\\?{:\\s][^\\/\\?{:\\s]*"
    },
    "path_param": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "PATTERN",
          "value": "[a-zA-Z_][a-zA-Z0-9_-]*"
        }
      ]
    },
    "variable": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "variable_delim_start"
        },
        {
          "type": "SYMBOL",
          "name": "variable_name"
        },
        {
          "type": "SYMBOL",
          "name": "variable_delim_end"
        }
      ]
    },
    "variable_delim_start": {
      "type": "STRING",
      "value": "{{"
    },
    "variable_delim_end": {
      "type": "STRING",
      "value": "}}"
    },
    "variable_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "query_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "?"
        },
        {
          "type": "SYMBOL",
          "name": "query_param"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "&"
              },
              {
                "type": "SYMBOL",
                "name": "query_param"
              }
            ]
          }
        }
      ]
    },
    "query_param": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "key"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "="
                },
                {
                  "type": "SYMBOL",
                  "name": "value"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "key": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "variable"
        },
        {
          "type": "PATTERN",
          "value": "[a-zA-Z0-9_-]+"
        }
      ]
    },
    "value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "variable"
        },
        {
          "type": "SYMBOL",
          "name": "path_segment"
        },
        {
          "type": "PATTERN",
          "value": "[^&]+"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [
    [
      "domain_and_port"
    ],
    [
      "path"
    ]
  ],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}